    "url": "git+https://github.com/ballerina-lang/ballerina.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ballerina-lang/ballerina/issues"
  },
  "homepage": "http://ballerinalang.org",
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "lerna": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.5.1",
    "shelljs": "0.8.1"
  },
  "dependencies": {
    "rimraf": "^2.6.2"
  }
