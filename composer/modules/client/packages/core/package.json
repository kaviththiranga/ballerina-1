{
  "name": "@ballerina-lang/composer-core",
  "version": "0.962.0",
  "description": "Core framework of Composer",
  "homepage": "https://github.com/ballerina-lang/ballerina/composer/modules/client/packages/core",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "bugs": {
    "url": "https://github.com/ballerina-lang/ballerina/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ballerina-lang/ballerina.git"
  },
  "scripts": {
    "clean": "rimraf lib",
    "dev": "nodemon --watch ./src --exec 'npm run build'",
    "copy-assets": "copyfiles -u 1 \"src/**/*.css\" \"src/**/*.scss\" \"src/**/*.json\" lib",
    "build": "npm run clean && npm run copy-assets && babel src -d lib -s"
  },
  "dependencies": {
    "@ballerina-lang/composer-api-client": "0.962.0",
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.0.8",
    "jquery": "^1.9.1",
    "lodash": "^4.13.1",
    "log4javascript": "^1.4.15",
    "mousetrap": "^1.6.0",
    "prop-types": "^15.5.8",
    "rc-menu": "^5.0.10",
    "rc-tabs": "^9.1.10",
    "react": "^16.2.0",
    "react-bootstrap": "^0.31.5",
    "react-contextmenu": "^2.9.0",
    "react-custom-scrollbars": "^4.1.2",
    "react-dnd": "^2.5.3",
    "react-dnd-html5-backend": "^2.5.3",
    "react-dom": "^16.2.0",
    "react-htmltree": "github:pahans/react-htmltree#upgrade-to-react-16",
    "react-notification-system": "^0.2.15",
    "react-split-pane": "^0.1.71",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "copyfiles": "^1.2.0",
    "nodemon": "^1.15.0",
    "rimraf": "^2.6.2"
  }
}
