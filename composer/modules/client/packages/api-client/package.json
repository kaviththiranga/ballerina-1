{
  "name": "@ballerina-lang/composer-api-client",
  "version": "0.962.0",
  "description": "A Client for the backend services of Composer",
  "homepage": "https://github.com/ballerina-lang/ballerina/composer/modules/client/packages/api-client",
  "license": "Apache-2.0",
  "main": "lib/api-client.js",
  "bugs": {
    "url": "https://github.com/ballerina-lang/ballerina/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ballerina-lang/ballerina.git"
  },
  "scripts": {
    "clean": "rimraf lib",
    "dev": "nodemon --watch ./src --exec 'npm run build'",
    "copy-assets": "copyfiles -u 1 \"src/**/*.css\" \"src/**/*.scss\" \"src/**/*.json\" lib",
    "build": "npm run clean && npm run copy-assets && babel src -d lib -s"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "jquery": "^1.9.1",
    "lodash": "^4.13.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "copyfiles": "^1.2.0",
    "nodemon": "^1.15.0",
    "rimraf": "^2.6.2"
  }
}
